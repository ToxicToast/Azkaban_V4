		"update:all": "npx npm-check-updates -u",
		"update:filtered": "npx npm-check-updates -u --filter @nestjs/*",
		"format:check": "nx format:check",
		"affected:lint": "nx affected:lint --parallel --maxParallel=3",
		"affected:test": "nx affected:test --parallel --maxParallel=3 --passWithNoTests",
		"affected:build": "nx affected:build --parallel --maxParallel=3",
		"lintstaged": "lint-staged",
		"husky:scripts": "npm run format:check && npm run affected:lint && npm run lintstaged",
		"upgrade:workspace": "nx migrate latest",
		"upgrade:migrations": "nx migrate --run-migrations",
		"report": "nx report",
		"serve:gateway": "nx serve gateway"
